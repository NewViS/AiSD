#include <iostream>
#include "arr_mn.h"
using namespace std;

int main() {
	ArrMn mn;
	const int N = 17;
	char b[] = "4561";
	char a[] = "1234";
	char *c;

	c = mn.intersect(a, b, c);

	for(int i = 0; c[i]; ++i){
		cout << c[i] << " ";
	}
	return 0;
}
