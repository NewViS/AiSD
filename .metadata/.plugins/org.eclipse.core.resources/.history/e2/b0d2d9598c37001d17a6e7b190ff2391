int main() {
	ArrMn mn;
	const int N = 17;
	char a[] = "abcde";
	char b[] = "bdf";
	char c[] = "dfgh";
	char d[] = "aci";
	char e[N];

	mn.unite(a, b, c);

	for(int i = 0; c[i]; ++i){
		cout << c[i] << " ";
	}
	return 0;
}
