#include <iostream>
#include "arr_mn.h"
using namespace std;

int main() {
	ArrMn a;
	ArrMn b;
	ArrMn c;
	char bc[] = "4561";
	char ac[] = "1234";
	a.setElem(ac);
	b.setElem(bc);

	c=a.intersect(a, b);

	cout << c << endl; // prints !!!Hello World!!!
	return 0;
}
